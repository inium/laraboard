{
    "name": "inium/laraboard",
    "description": "A simple board on Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "board"],
    "authors": [
        {
            "name": "inlee",
            "email": "jamesinlee@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "jenssegers/agent": "^3.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inium\\Laraboard\\LaraboardServiceProvider"
            ],
            "aliases": {
                "LaraboardAgent": "Inium\\Laraboard\\Support\\Facades\\Agent"
            }
        },
        "branch-alias": {
            "dev-main": "0.2.0-dev"
        }
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Inium\\Laraboard\\": "src/"
        }
    }
}
